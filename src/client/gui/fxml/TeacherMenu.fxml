<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.*?>
<SplitPane xmlns:fx="http://javafx.com/fxml/1" dividerPositions="0.5" xmlns="http://javafx.com/javafx/9"
           fx:controller="client.gui.fxcontrol.TeacherMenu">
    <items>
        <AnchorPane prefHeight="400.0" prefWidth="600.0">
            <children>
                <AnchorPane fx:id="welcomeScreen" prefHeight="200.0" prefWidth="200.0"
                            style="-fx-background-image: url(&quot;../../Icon.png&quot;); -fx-background-position: center center;"
                            AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                            AnchorPane.topAnchor="0.0"/>
                <AnchorPane fx:id="questionPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                            minHeight="-Infinity" minWidth="-Infinity" prefHeight="180.0" prefWidth="200.0"
                            AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                            AnchorPane.topAnchor="0.0">

                        </AnchorPane>
                <AnchorPane fx:id="examPane" minHeight="-Infinity" minWidth="-Infinity" prefHeight="180.0"
                            prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                            AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
            </children>
        </AnchorPane>
        <AnchorPane maxWidth="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="120.0">
            <children>
                <Button mnemonicParsing="false" onAction="#switchToQuestionView" text="Questions"
                        AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0"/>
                <Button layoutX="29.0" layoutY="35.0" mnemonicParsing="false" onAction="#switchToExamView" text="Exams"
                        AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="40.0"/>
            </children>
        </AnchorPane>
    </items>
</SplitPane>
